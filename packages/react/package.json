{
  "name": "@verygoodgraphics/vgg-react",
  "homepage": "https://verygoodgraphics.com/",
  "repository": {
    "type": "git",
    "url": "https://github.com/verygoodgraphics/vgg_react.git"
  },
  "version": "0.1.12",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/vgg-react.umd.cjs",
  "module": "./dist/vgg-react.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/vgg-react.js",
      "require": "./dist/vgg-react.umd.cjs"
    }
  },
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview"
  },
  "dependencies": {
    "@verygoodgraphics/vgg-wasm": "workspace:^",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/offscreencanvas": "^2019.7.3",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "@vitejs/plugin-react": "^4.2.0",
    "eslint": "^8.53.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.4",
    "typescript": "^5.2.2",
    "vite": "^5.0.0",
    "vite-plugin-dts": "^3.6.3"
  }
}
